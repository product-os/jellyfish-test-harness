{
  "name": "@balena/jellyfish-test-harness",
  "version": "6.0.55",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-test-harness.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Test harness helpers for Jellyfish repos",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib",
    "unit": "jest",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-core": "^5.1.11",
    "@balena/jellyfish-environment": "^4.3.18",
    "@balena/jellyfish-plugin-base": "^2.1.210",
    "@balena/jellyfish-queue": "^1.0.233",
    "@balena/jellyfish-sync": "^6.1.1",
    "@balena/jellyfish-worker": "^8.0.11",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "errio": "^1.2.2",
    "express": "^4.17.1",
    "js-combinatorics": "^1.5.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nock": "^13.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.4.8",
    "@balena/jellyfish-types": "^0.8.12",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.6",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.5.1",
    "ts-jest": "^27.0.4",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-07-31T03:16:02.450Z"
  }
}
