{
  "name": "@balena/jellyfish-test-harness",
  "version": "8.1.177",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-test-harness.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Test harness helpers for Jellyfish repos",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib",
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-core": "^8.1.43",
    "@balena/jellyfish-environment": "^5.2.2",
    "@balena/jellyfish-plugin-base": "^2.1.317",
    "@balena/jellyfish-queue": "^1.0.379",
    "@balena/jellyfish-sync": "^6.1.125",
    "@balena/jellyfish-worker": "^10.0.143",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "errio": "^1.2.2",
    "express": "^4.17.1",
    "js-combinatorics": "^1.5.4",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nock": "^13.2.1",
    "random-words": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.1",
    "@balena/jellyfish-types": "^1.2.0",
    "@balena/lint": "^6.2.0",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.3",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-12-05T17:51:25.570Z"
  }
}
